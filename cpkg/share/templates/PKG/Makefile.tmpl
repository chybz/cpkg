#
# cpkg generated Makefile
#
# DO NOT EDIT
#
SHELL=/bin/bash
CPKG=%{ $CPKG_BIN }%

.SILENT:

all: build

build:
	$(CPKG) build

install:
	$(CPKG) install

clean:
	rm -rf %{ $PKG_NAME }%-*
% if (($CPKG_IS_DEB)); then
	rm -f *.deb *.changes *.upload
% fi

% if (($CPKG_IS_DEB)); then
package: clean
	$(CPKG) package
% fi

update-pkg: clean
	$(CPKG) package -P -I

% if (($CPKG_IS_PKGSRC)); then
update-pkg-full: clean
	$(CPKG) package -P -I -f
% fi
