#
# cpkg generated Makefile
#
# DO NOT EDIT
#
SHELL=/bin/bash
CPKG=%{ $CPKG_BIN }%

.SILENT:

all: build

build:
	$(CPKG) build

% if (($CPKG_IS_PKGSRC)); then
install:
	$(CPKG) install
% fi

clean:
	rm -rf %{ $PKG_NAME }%-*
% if (($CPKG_IS_DEB)); then
	rm -f *.deb *.changes *.upload *.buildinfo
% fi

% if (($CPKG_IS_DEB)); then
package: clean build
	$(CPKG) package

upload:
	dput *.changes
% fi

update-pkg: clean
	$(CPKG) package -P -I
